# RaceSense Production Environment Configuration

# Application
VITE_APP_NAME=RaceSense
VITE_APP_VERSION=1.2.0
VITE_APP_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://api.racesense.app
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Analytics & Monitoring
VITE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://your-dsn@sentry.io/project
VITE_HOTJAR_ID=your-hotjar-id

# Racing Services
VITE_TELEMETRY_WEBSOCKET_URL=wss://telemetry.racesense.app
VITE_GPS_SERVICE_URL=https://gps.racesense.app
VITE_WEATHER_API_KEY=your-weather-api-key

# Cloud Storage
VITE_AWS_REGION=us-east-1
VITE_AWS_BUCKET=racesense-production
VITE_CDN_URL=https://cdn.racesense.app

# Social & Integrations
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-key
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SW=true
VITE_CACHE_VERSION=v1.2.0

# Security
VITE_CSP_NONCE=production-csp-nonce
VITE_ENABLE_HTTPS_ONLY=true

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_MOBILE_FEATURES=true
VITE_ENABLE_COMMERCIAL_FEATURES=true
VITE_ENABLE_TELEMETRY_EXPORT=true
